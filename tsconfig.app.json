{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],

    "paths": {
      "@/*": ["./src/*"]
    },
    "types": [
      "vue-macros/macros-global",
      "element-plus/global",
      "vite/client"
    ]
  },
  "include": [
    "src/**/*",
    "src/**/*.vue",
    "types/**/*.d.ts"
  ],
  "vueCompilerOptions": {
    "plugins": [
      "vue-macros/volar"
    ]
  }
}
